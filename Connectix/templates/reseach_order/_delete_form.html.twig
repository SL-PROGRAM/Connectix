<form method="post" action="{{ path('reseach_order_delete', {'id': reseach_order.id}) }}" onsubmit="return confirm({{ 'button.confirm.delete.researchorder' | trans }});">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ reseach_order.id) }}">
    <button class="btn">{{ 'button.delete' | trans }}</button>
</form>
